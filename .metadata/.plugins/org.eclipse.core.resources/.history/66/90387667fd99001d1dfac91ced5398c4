package logical_Programming;

import java.util.Scanner;

public class PrimeNumber {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		System.out.println("please enter the number :");
		
		int a = sc.nextInt();
		
		int co;
		
		for(int i =1;i<=a;i++)
		{
			if(a%i==0)
			{
				co++;
			}
		}
	}

}
